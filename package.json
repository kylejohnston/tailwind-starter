  "scripts": {
    "serve": "cross-env NODE_ENV=development concurrently \"postcss css/tailwind.css -o public/css/styles.css --watch\" \"live-server public -q\"",
    "development": "cross-env NODE_ENV=development postcss css/tailwind.css -o public/css/styles.css",
    "dev-watch": "cross-env NODE_ENV=development postcss css/tailwind.css -o public/css/styles.css --watch",
    "production": "cross-env NODE_ENV=production postcss css/tailwind.css -o public/css/styles.min.css",
    "start": "npm run serve"
  },
  "devDependencies": {
    "cssnano": "^4.1.10"
